CONST
    false = 0
    true = 1
VAR
    a b
BEGIN
    READ a;
    READ b;
    IF a >= b THEN
        WRITE true;
        READ a;
        READ b;
        IF a >= b THEN
            WRITE true;
        ELSE
            WRITE false;
        END
    ELSE
        WRITE false;
        READ a;
        READ b;
        IF a >= b THEN
            WRITE true;
        ELSE
            WRITE false;
        END
    END
    a := a + b;
    WRITE a;
END
